{
  "name": "altamoon",
  "version": "0.0.363",
  "private": true,
  "productName": "Altamoon",
  "description": "Binance trading",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.main.prod.babel.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.renderer.prod.babel.js",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint-pure": "cross-env NODE_ENV=development eslint ./src ./app --ext .js,.jsx,.ts,.tsx",
    "lint": "npm run lint-pure -- --cache --fix",
    "package": "rm -rf src/dist && npm run build && electron-builder build --publish never",
    "postinstall": "node -r @babel/register .erb/scripts/CheckNativeDep.js && electron-builder install-app-deps && node ./scripts/postinstall.js",
    "start": "node -r @babel/register .erb/scripts/CheckPortInUse.js && cross-env npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ./.erb/scripts/BabelRegister ./src/main.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./.erb/configs/webpack.config.renderer.dev.babel.js",
    "start:browser": "cross-env NODE_ENV=development webpack serve --config ./.erb/configs/webpack.config.browser.babel.js --stats-children",
    "build:browser": "cross-env NODE_ENV=production webpack --config ./.erb/configs/webpack.config.browser.babel.js",
    "tsc": "npx tsc --noemit",
    "test": "npm run lint-pure && npx tsc --noemit",
    "apidocs": "npx typedoc --out  packages/altamoon-binance-api/docs app/api/*.ts",
    "upgrade": "npx npm-check-updates -u -x chalk,eslint-config-airbnb-typescript,react-router-dom --timeout 480000 && npm i --legacy-peer-deps",
    "reinstall": "rm -rf node_modules package-lock.json && npm i --legacy-peer-deps",
    "build-types": "rm -rf ./packages/altamoon-types/_tmp && npx tsc --declaration --outDir ./packages/altamoon-types/_tmp --emitDeclarationOnly && cp -a ./packages/altamoon-types/_tmp/app/. ./packages/altamoon-types/types/ && rm -r ./packages/altamoon-types/_tmp && node scripts/copyProjectVersion",
    "build-api": "npx tsc ./app/api/* --declaration --esModuleInterop --skipLibCheck --outDir ./packages/altamoon-binance-api/api --target ES2021 --module ES2020 --moduleResolution node && node scripts/copyProjectVersion",
    "publish-types": "npm publish ./packages/altamoon-types/",
    "publish-api": "npm publish ./packages/altamoon-binance-api/",
    "patch:env": "npm version patch && npm run build-types && npm run build-api && npm run build:browser && git commit -am 'chore: Build types, API and browser version' && npm run publish-types && npm run publish-api && git push && git push --tags",
    "patch": "rm -rf dist-dev && DIST_ENV=development deploy-to-git --repository git@github.com:Altamoon/altamoon-web-dev.git --folder dist-dev",
    "patch:prod": "rm -rf dist-prod && DIST_ENV=production deploy-to-git --repository git@github.com:Altamoon/altamoon-web-prod.git --folder dist-prod"
  },
  "config": {
    "deployToGit": {
      "branch": "main",
      "script": "npm run patch:env",
      "commit": "Automatic commit text",
      "user": {
        "email": "github@altamoon.io",
        "name": "Altamoon"
      }
    }
  },
  "build": {
    "productName": "Altamoon",
    "appId": "org.altamoon",
    "files": [
      "dist/",
      "node_modules/",
      "index.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "afterSign": ".erb/scripts/Notarize.js",
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "src",
      "buildResources": "assets",
      "output": "release"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "electron-react-boilerplate",
      "repo": "electron-react-boilerplate"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Altamoon/altamoon.git"
  },
  "author": {
    "name": "Letiliel",
    "email": "dev@xod.me",
    "url": "https://github.com/Letiliel"
  },
  "contributors": [
    {
      "name": "Andrey Gubanov",
      "email": "andrey@gubanov.eu",
      "url": "https://opensource.gubanov.eu/"
    }
  ],
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/Altamoon/altamoon/issues"
  },
  "homepage": "https://github.com/Altamoon/altamoon#readme",
